<mapper namespace="com.qianqian.manager.dao.CategoryMapper" >

  <select id="countProducts" resultType="_long">
    SELECT COUNT(*) FROM category
  </select>

  <select id="findCategory" resultType="com.qianqian.manager.pojo.po.Category">
    select cid,cname from category
  </select>

